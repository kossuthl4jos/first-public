    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^2.0.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
