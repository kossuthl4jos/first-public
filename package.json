{
  "name": "my-vue-webpack",
  "description": "sample",
  "author": "Lajos",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "config": {
    "default_public_path": "/"
  },
  "scripts": {
    "bundle": "rimraf public && webpack --progress --hide-modules --config build/webpack.config",
    "dev": "webpack-dev-server --host 0.0.0.0 --disable-host-check --mode development --config build/webpack.config",
    "build": "cross-env NODE_ENV=production npm run bundle -- --mode production",
    "analyze": "cross-env ANALYZE=true npm run build",
    "prod": "cross-env NODE_ENV=production node server",
    "test": "npm run lint && npm run unit -- --coverage --verbose",
    "lint": "eslint \"@(src|test|build)/**/*.@(js|vue)\"",
    "unit": "cross-env NODE_ENV=test jest",
    "unit:watch": "npm run unit -- --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.4.1",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "compression": "^1.7.3",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "express-history-api-fallback": "^2.2.1",
    "express-http-proxy": "^1.2.0",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "odometer": "^0.4.8",
    "rimraf": "^2.6.2",
    "tap": "^12.0.1",
    "uuid": "^3.3.2",
    "uuid-validate": "0.0.3",
    "vue-odometer": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "buefy": "^0.6.7",
    "bulma": "^0.7.1",
    "css-loader": "^1.0.0",
    "eslint": "^5.6.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^1.1.1",
    "vue": "^2.5.13",
    "vue-focus-lock": "^1.2.0",
    "vue-i18n": "^8.1.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  }
}
