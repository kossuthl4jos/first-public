{
  "name": "my-vue-webpack",
  "description": "sample",
  "author": "Lajos",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "config": {
    "default_public_path": "/"
  },
  "scripts": {
    "bundle": "webpack --progress --hide-modules --config build/webpack.config",
    "dev": "webpack-dev-server --host 0.0.0.0 --disable-host-check --mode development --config build/webpack.config",
    "build": "cross-env NODE_ENV=production npm run bundle -- --mode production",
    "analyze": "cross-env ANALYZE=true npm run build",
    "prod": "cross-env NODE_ENV=production node server",
    "test": "npm run unit",
    "unit": "cross-env NODE_ENV=test karma start karma.config",
    "unit:watch": "npm run unit -- --no-single-run"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^5.1.4",
    "express": "^4.16.3",
